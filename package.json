{
  "name": "cnct",
  "version": "0.2.0-prerelease",
  "description": "",
  "private": true,
  "main": "dist/index.js",
  "scripts": {
    "compile": "node ./node_modules/typescript/bin/tsc",
    "lint": "node ./node_modules/tslint/bin/tslint -p tsconfig.json -c tslint.json src/**/*.ts test/**/*.ts",
    "mocha": "node ./node_modules/mocha/bin/mocha",
    "cover": "node ./node_modules/nyc/bin/nyc.js --reporter=lcov --reporter=text-summary ./node_modules/mocha/bin/_mocha",
    "codacy": "node ./node_modules/shx/lib/cli.js cat ./coverage/lcov.info | node ./node_modules/codacy-coverage/bin/codacy-coverage.js --language typescript",
    "test": "npm run lint && npm run compile && npm run cover && npm run codacy"
  },
  "repository": "git+https://github.com/bgold09/cnct.git",
  "keywords": [
    "dotfiles",
    "cli"
  ],
  "author": "Brian Golden",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/bgold09/cnct/issues"
  },
  "homepage": "https://github.com/bgold09/cnct#readme",
  "nyc": {
    "include": [
      "src/**/*.ts"
    ],
    "exclude": [
      "src/**/ConsoleLogger.ts"
    ],
    "extension": [
      ".ts"
    ],
    "reporter": [
      "html"
    ],
    "all": true,
    "sourceMap": true,
    "instrument": true
  },
  "devDependencies": {
    "@types/chai": "^4.1.6",
    "@types/chai-as-promised": "^7.1.0",
    "@types/command-line-args": "^4.0.2",
    "@types/mocha": "^5.2.5",
    "@types/node": "^9.6.2",
    "@types/node-powershell": "^3.1.0",
    "@types/rimraf": "^2.0.2",
    "@types/shelljs": "^0.8.0",
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1",
    "codacy-coverage": "^2.0.3",
    "mocha": "^5.2.0",
    "nyc": "^11.6.0",
    "shx": "^0.3.2",
    "source-map-support": "^0.5.9",
    "ts-node": "^5.0.1",
    "tslint": "^5.11.0",
    "tslint-microsoft-contrib": "^5.0.3",
    "typemoq": "^2.1.0",
    "typescript": "^2.9.2"
  },
  "dependencies": {
    "class-transformer": "^0.1.10",
    "command-line-args": "^5.0.2",
    "command-line-usage": "^5.0.5",
    "node-powershell": "^3.3.1",
    "rimraf": "^2.6.2",
    "shelljs": "^0.8.2"
  }
}
